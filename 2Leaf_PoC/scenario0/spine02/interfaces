auto lo
iface lo inet loopback
    address 10.0.0.52/32

auto eth0
iface eth0 inet dhcp

auto swp1
iface swp1
  post-up ip link set $IFACE promisc on

auto swp32s0
iface swp32s0
  post-up ip link set $IFACE promisc on

auto swp32s1
iface swp32s1
  post-up ip link set $IFACE promisc on
 
auto swp32s2
iface swp32s2
  post-up ip link set $IFACE promisc on

auto swp32s3
iface swp32s3
  post-up ip link set $IFACE promisc on

auto swp20
iface swp20
  post-up ip link set $IFACE promisc on

auto swp21
iface swp21
  post-up ip link set $IFACE promisc on
 
auto bridge
iface bridge
  bridge-ports leafbond peerlink ciscobond
  bridge-vlan-aware yes
  bridge-stp on
  bridge-vids 1-1000
  bridge-pvid 1
  mstpctl-treeprio 16384

auto bridge.10
iface bridge.10
  address 10.10.0.52/24

auto bridge.20
iface bridge.20  
  address 10.20.0.52/24
   
auto peerlink
iface peerlink
  bond-slaves swp1
  bond-miimon 100
  bond-mode 802.3ad  
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1
    
auto peerlink.4094
iface peerlink.4094
  address 169.254.1.2/30
  clagd-enable yes
  clagd-peer-ip 169.254.1.1
  clagd-backup-ip 10.10.0.51
  clagd-sys-mac 44:38:39:FF:40:93    
  
auto leafbond
iface leafbond
  bond-slaves swp32s0 swp32s1 swp32s2 swp32s3
  bond-miimon 100
  bond-mode 802.3ad  
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1  
  clag-id 1

auto ciscobond
iface ciscobond
  bond-slaves swp20 swp21
  bond-miimon 100
  bond-mode 802.3ad
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1  
  clag-id 2 
  