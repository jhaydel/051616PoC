auto lo
iface lo inet loopback
    address 10.0.0.12/32

auto eth0
iface eth0 inet dhcp

auto swp1
iface swp1
  post-up ip link set $IFACE promisc on

auto swp2
iface swp2
  post-up ip link set $IFACE promisc on

auto swp5
iface swp5
  post-up ip link set $IFACE promisc on
  
auto swp6
iface swp6
  post-up ip link set $IFACE promisc on
  
auto swp9
iface swp9
  post-up ip link set $IFACE promisc on
  
auto swp10
iface swp10
  post-up ip link set $IFACE promisc on

auto swp13
iface swp13
  post-up ip link set $IFACE promisc on

auto swp14
iface swp14
  post-up ip link set $IFACE promisc on
            
auto swp43
iface swp43
  post-up ip link set $IFACE promisc on

auto swp44
iface swp44
  post-up ip link set $IFACE promisc on

auto swp45
iface swp45
  post-up ip link set $IFACE promisc on
  
auto swp46
iface swp46
  post-up ip link set $IFACE promisc on
 
auto swp48
iface swp48
  post-up ip link set $IFACE promisc on
  
auto bridge
iface bridge
  bridge-ports spinebond peerlink hostbond1 hostbond2 hostbond3
  bridge-vlan-aware yes
  bridge-stp on
  bridge-vids 1-1000
  bridge-pvid 1

auto bridge.10
iface bridge.10
  address 10.10.0.12/24

auto bridge.20
iface bridge.20  
  address 10.20.0.12/24
 
auto peerlink
iface peerlink
  bond-slaves swp48
  bond-miimon 100
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1
    
auto peerlink.4094
iface peerlink.4094
  address 169.254.1.2/30
  clagd-enable yes
  clagd-peer-ip 169.254.1.1
  clagd-backup-ip 10.10.0.11
  clagd-sys-mac 44:38:39:FF:40:94    
  
auto spinebond
iface spinebond
  bond-slaves swp43 swp44 swp45 swp46
  bond-miimon 100
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1  
  clag-id 1 

auto hostbond1
iface hostbond1
  bond-slaves swp1 swp2
  bond-miimon 100
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1  
  clag-id 2
  
auto hostbond2
iface hostbond2
  bond-slaves swp5 swp6
  bond-miimon 100
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1  
  clag-id 3

auto hostbond3
iface hostbond3
  bond-slaves swp9 swp10
  bond-miimon 100
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
  bond-min-links 1  
  clag-id 4   

auto hostbond4
iface hostbond4
  bond-slaves swp13 swp14
  bond-miimon 100
  bond-use-carrier 1
  bond-xmit-hash-policy layer3+4
  bond-lacp-rate 1
